<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/event_tags_bottom_view"
    android:background="@drawable/filter_background_shape"
    android:paddingVertical="20dp"
    >
    <View
        android:id="@+id/view7"
        android:layout_width="match_parent"
        android:layout_height="4dp"
        android:layout_marginHorizontal="140dp"
        android:background="@drawable/rounded_borders_no_color"
        android:backgroundTint="#777777"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.022" />

    <TextView
        android:id="@+id/dance_textView"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:background="@drawable/rounded_borders_no_color"
        android:backgroundTint="@color/red"
        android:fontFamily="@font/poppins"
        android:gravity="center"
        android:paddingHorizontal="18dp"
        android:paddingVertical="7dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="3dp"
        android:text="Dance"
        android:textColor="@color/white"
        android:textSize="13sp"
        app:layout_constraintEnd_toStartOf="@+id/karaoke_textView"
        app:layout_constraintTop_toBottomOf="@+id/view7" />

    <TextView
        android:id="@+id/karaoke_textView"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="5dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/rounded_borders_no_color"
        android:backgroundTint="@color/red"
        android:fontFamily="@font/poppins"
        android:gravity="center"
        android:paddingHorizontal="18dp"
        android:paddingVertical="7dp"
        android:text="Karaoke"
        android:textColor="@color/white"
        android:textSize="13sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view7" />

    <TextView
        android:id="@+id/rave_textView"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:background="@drawable/rounded_borders_no_color"
        android:backgroundTint="@color/red"
        android:fontFamily="@font/poppins"
        android:gravity="center"
        android:paddingHorizontal="18dp"
        android:paddingVertical="7dp"
        android:layout_marginStart="3dp"
        android:text="Rave"
        android:textColor="@color/white"
        android:textSize="13sp"
        app:layout_constraintStart_toEndOf="@+id/karaoke_textView"
        app:layout_constraintTop_toBottomOf="@+id/view7" />

    <TextView
        android:id="@+id/disco_textView"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:background="@drawable/rounded_borders_no_color"
        android:backgroundTint="@color/red"
        android:fontFamily="@font/poppins"
        android:gravity="center"
        android:paddingHorizontal="18dp"
        android:layout_marginTop="15dp"
        android:paddingVertical="7dp"
        android:layout_marginStart="5dp"
        android:layout_marginEnd="3dp"
        android:text="Disco"
        android:textColor="@color/white"
        android:textSize="13sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dance_textView" />

    <TextView
        android:id="@+id/rock_textView"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:background="@drawable/rounded_borders_no_color"
        android:backgroundTint="@color/red"
        android:fontFamily="@font/poppins"
        android:gravity="center"
        android:paddingHorizontal="18dp"
        android:paddingVertical="7dp"
        android:layout_marginHorizontal="3dp"
        android:text="Rock"
        android:layout_marginTop="15dp"
        android:textColor="@color/white"
        android:textSize="13sp"
        app:layout_constraintStart_toEndOf="@+id/disco_textView"
        app:layout_constraintTop_toBottomOf="@+id/karaoke_textView" />

    <TextView
        android:id="@+id/funk_textView"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="3dp"
        android:background="@drawable/rounded_borders_no_color"
        android:backgroundTint="@color/red"
        android:fontFamily="@font/poppins"
        android:gravity="center"
        android:paddingHorizontal="18dp"
        android:paddingVertical="7dp"
        android:text="Funk"
        android:layout_marginTop="15dp"
        android:textColor="@color/white"
        android:textSize="13sp"
        app:layout_constraintStart_toEndOf="@+id/rock_textView"
        app:layout_constraintTop_toBottomOf="@+id/karaoke_textView" />

    <TextView
        android:id="@+id/pop_textView"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="3dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/rounded_borders_no_color"
        android:backgroundTint="@color/red"
        android:fontFamily="@font/poppins"
        android:gravity="center"
        android:paddingHorizontal="18dp"
        android:paddingVertical="7dp"
        android:text="Pop"
        android:textColor="@color/white"
        android:textSize="13sp"

        app:layout_constraintStart_toEndOf="@+id/funk_textView"
        app:layout_constraintTop_toBottomOf="@+id/rave_textView" />

    <TextView
        android:id="@+id/rap_textView"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:background="@drawable/rounded_borders_no_color"
        android:backgroundTint="@color/red"
        android:fontFamily="@font/poppins"
        android:gravity="center"
        android:paddingHorizontal="18dp"
        android:paddingVertical="7dp"
        android:text="Rap"
        android:layout_marginTop="15dp"
        android:textColor="@color/white"
        android:textSize="13sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/disco_textView" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:background="@drawable/rounded_borders_no_color"
        android:backgroundTint="@color/red"
        android:fontFamily="@font/poppins"
        android:gravity="center"
        android:paddingHorizontal="18dp"
        android:paddingVertical="7dp"
        android:text="test"
        android:textColor="@color/white"
        android:textSize="13sp"
        android:layout_marginHorizontal="3dp"
        android:layout_marginTop="15dp"
        app:layout_constraintStart_toEndOf="@+id/rap_textView"
        app:layout_constraintTop_toBottomOf="@+id/funk_textView" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:background="@drawable/rounded_borders_no_color"
        android:backgroundTint="@color/red"
        android:fontFamily="@font/poppins"
        android:gravity="center"
        android:paddingHorizontal="18dp"
        android:paddingVertical="7dp"
        android:text="test"
        android:textColor="@color/white"
        android:textSize="13sp"
        android:layout_marginHorizontal="3dp"
        android:layout_marginTop="15dp"
        app:layout_constraintEnd_toStartOf="@+id/rap_textView"
        app:layout_constraintTop_toBottomOf="@+id/disco_textView" />

</androidx.constraintlayout.widget.ConstraintLayout>